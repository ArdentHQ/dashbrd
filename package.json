{
    "private": true,
    "engines": {
        "node": ">=18.11.0"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev": "vite",
        "docker": "docker-compose down -t 0 -v && docker-compose up -V",
        "build": "vite build",
        "format": "pnpm lint && pnpm prettier",
        "lint": "eslint \"resources/**/*.{js,ts,jsx,tsx}\" --fix",
        "prettier": "prettier \"./*.{ts,tsx,js,json,md,mdx}\" \"{resources,stories}/**/*.{js,ts,jsx,tsx,mdx}\" --write",
        "generate:typescript": "php artisan typescript:transform",
        "postgenerate:typescript": "prettier  \"resources/types/**/*.{js,ts,jsx,tsx}\" --write",
        "generate:db": "php artisan migrate:fresh --seed",
        "check-types": "pnpm exec tsc --noEmit",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "test": "REACT_APP_IS_UNIT=true vitest --update",
        "test:coverage": "REACT_APP_IS_UNIT=true vitest run --coverage",
        "test:coverage-components": "REACT_APP_IS_UNIT=true COVERAGE_THRESHOLD_LINES=100 COVERAGE_THRESHOLD_FUNCTIONS=100 COVERAGE_THRESHOLD_STATEMENTS=100 COVERAGE_THRESHOLD_BRANCHES=100 COVERAGE_INCLUDE_PATH=resources/js/Components vitest run --coverage resources/js/Components",
        "prepare": "husky install"
    },
